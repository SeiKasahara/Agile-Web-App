{

    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.fixAll.eslint": "explicit"
    },
  
    "eslint.validate": [
      "javascript",
      "javascriptreact"
    ],
  
    "stylelint.validate": [
      "css"
    ],

    "htmlhint.enable": true,

    "files.associations": {
      "*.html": "html",
      "*.css": "css",
      "*.js": "javascript"
    },
  
  }
  